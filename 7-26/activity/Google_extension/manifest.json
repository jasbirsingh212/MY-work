{
    "name": "My Extension",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "background": {
        "scripts": ["backgroun.js"],
        "persistent": false
      },
      "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
      "browser_action" : {
          "default_popup" : "popup.html"
      },

      "web_accessible_resources": [
        "images/*.png",
        "images/*.PNG"
      ],

      "permissions" : [
        "tabs"
      ]
  }